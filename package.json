{
  "name": "travelspark",
  "version": "0.0.2",
  "description": "Monorepo for the front and back ends of TravelSpark, an AI-powered travel application",
  "main": "/server/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "server:dev": "nodemon ./server/app.js",
    "server:prod": "export NODE_ENV=production; node ./server/app.js",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "db:create": "sequelize-cli db:create",
    "db:migrate": "sequelize-cli db:migrate",
    "db:drop": "sequelize-cli db:drop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anth-volk/travelspark.git"
  },
  "keywords": [
    "ai",
    "artificial-intelligence",
    "travel",
    "react",
    "node",
    "express"
  ],
  "author": "Anthony Volk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anth-volk/travelspark/issues"
  },
  "homepage": "https://github.com/anth-volk/travelspark#readme",
  "devDependencies": {
    "auto-changelog": "^2.4.0",
    "create-react-app": "^5.0.1",
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.1"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.11.2",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.32.1"
  }
}
